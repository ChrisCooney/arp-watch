<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>com.github.heywoodlh.arpwatch</string>

        <key>ProgramArguments</key>
        <array>
		<string>/usr/local/bin/arpwatch</string>
		<string>--outfile=/var/log/arpwatch.log</string>
        </array>
	
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
</dict>
</plist>
